{
  "name": "bde-isima",
  "appName": "BDE ISIMA",
  "website": "https://bde.isima.fr",
  "version": "4.2.0",
  "scripts": {
    "dev": "npx blitz dev",
    "build": "npx blitz build",
    "start": "npx blitz start",
    "studio": "npx blitz prisma studio",
    "seed": "prisma migrate reset --skip-generate --skip-seed && prisma db push && prisma db seed",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node db/seeds/index.ts"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.3",
    "@prisma/client": "^3.2.1",
    "bde-isima-mui-rff": "^4.0.0",
    "blitz": "^0.41.0",
    "cuid": "2.1.8",
    "date-fns": "^2.25.0",
    "filefy": "^0.1.11",
    "final-form": "^4.20.4",
    "final-form-arrays": "^3.0.2",
    "formidable": "1.2.2",
    "handlebars": "^4.7.7",
    "md5": "^2.3.0",
    "mjml": "^4.10.4",
    "mjml-react": "^2.0.0",
    "next-connect": "^0.10.2",
    "next-pwa": "^5.3.1",
    "nodemailer": "^6.6.5",
    "nprogress": "0.2.0",
    "prisma": "^3.2.1",
    "react": "^18.0.0-alpha-5fa4d79b0-20211008",
    "react-dom": "^18.0.0-alpha-5fa4d79b0-20211008",
    "react-error-boundary": "^3.1.3",
    "react-final-form": "^6.5.7",
    "react-final-form-arrays": "^3.1.3",
    "react-messenger-customer-chat": "0.8.0",
    "react-multi-carousel": "^2.6.5",
    "react-palette": "^1.0.2",
    "react-swipeable": "^6.2.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "1.8.6",
    "sanitize-html": "^2.5.1",
    "typescript": "^4.4.4",
    "victory": "^36.0.1",
    "zod": "^3.9.8"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/node": "^16.10.3",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.33",
    "autoprefixer": "^10.3.7",
    "critters": "0.0.10",
    "eslint": "^7.32.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.2",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "prettier-plugin-prisma": "^3.1.1",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^2.2.16",
    "ts-node": "^10.2.1"
  }
}
