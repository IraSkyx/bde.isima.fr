{
  "name": "bde-isima",
  "appName": "BDE ISIMA",
  "website": "https://bde.isima.fr",
  "version": "4.3.0",
  "scripts": {
    "build": "blitz build",
    "dev": "blitz dev",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint **/*.{js,ts,jsx} --fix",
    "migrate": "prisma db push",
    "postinstall": "husky install",
    "seed": "prisma migrate reset --skip-generate --skip-seed && prisma db push && prisma db seed",
    "start": "blitz start",
    "studio": "blitz prisma studio"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node db/seeds/index.ts"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 100,
    "bracketSpacing": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.88",
    "@mui/material": "^5.8.6",
    "@prisma/client": "^4.0.0",
    "blitz": "^0.45.4",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "filefy": "^0.1.11",
    "final-form": "^4.20.7",
    "final-form-arrays": "^3.0.2",
    "formidable": "^2.0.1",
    "handlebars": "^4.7.7",
    "md5": "^2.3.0",
    "mjml": "^4.12.0",
    "mjml-react": "^2.0.8",
    "mui-rff": "^5.3.0",
    "next-connect": "^0.12.2",
    "next-pwa": "^5.5.4",
    "nodemailer": "^6.7.6",
    "nprogress": "^0.2.0",
    "prisma": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.3",
    "react-messenger-customer-chat": "^0.8.0",
    "react-multi-carousel": "^2.8.2",
    "react-palette": "^1.0.2",
    "react-swipeable": "^7.0.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "sanitize-html": "^2.7.0",
    "typescript": "^4.7.4",
    "victory": "^36.5.3",
    "zod": "^3.17.3",
    "base64url": "^3.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/node": "^16.11.42",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.14",
    "autoprefixer": "^10.4.7",
    "critters": "^0.0.16",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-prisma": "^3.14.0",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1"
  },
  "engines": {
    "node": "16.x"
  },
  "engineStrict": true,
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "pretty-quick --staged"
    ]
  }
}
