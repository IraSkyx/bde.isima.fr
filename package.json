    "@types/faker": "^5.5.8",
    "@types/node": "^16.10.1",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.24",
    "autoprefixer": "^10.3.5",
    "eslint": "^7.32.0",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "prettier-plugin-prisma": "^3.1.1",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^2.2.15"
  }
}
