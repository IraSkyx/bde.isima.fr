    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.1",
    "@prisma/client": "^3.1.1",
    "blitz": "^0.40.0-canary.9",
    "cuid": "2.1.8",
    "date-fns": "^2.24.0",
    "filefy": "^0.1.11",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "formidable": "1.2.2",
    "handlebars": "^4.7.7",
    "md5": "^2.3.0",
    "mdi-material-ui": "^6.22.1",
    "mjml": "^4.10.3",
    "mjml-react": "^2.0.0",
    "mui-rff": "^3.0.16",
    "next-connect": "^0.10.2",
    "next-pwa": "^5.3.1",
    "nodemailer": "^6.6.5",
    "nprogress": "0.2.0",
    "prisma": "^3.1.1",
    "react": "0.0.0-experimental-b1a1cb116-20210924",
    "react-dom": "0.0.0-experimental-b1a1cb116-20210924",
    "react-error-boundary": "^3.1.3",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-messenger-customer-chat": "0.8.0",
    "react-multi-carousel": "^2.6.5",
    "react-palette": "^1.0.2",
    "react-swipeable": "^6.2.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "1.8.6",
    "sanitize-html": "^2.5.1",
    "typescript": "^4.4.3",
    "victory": "^36.0.1",
    "zod": "^3.8.2"
